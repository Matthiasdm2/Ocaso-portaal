    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@types/node": "20.14.12",
    "@types/react": "18.2.66",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "10.4.19",
